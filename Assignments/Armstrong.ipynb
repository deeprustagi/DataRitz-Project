    "def isarmstrong(n):\n",
    "    le=len(str(n)) \n",
    "    nc = n\n",
    "    val = 0\n",
    "    while (nc!=0): \n",
    "        rem = nc%10\n",
    "        val = val + pow(rem, le) \n",
    "        nc = nc//10\n",
    "    return (val == n) \n",
    "\n",
    "n=int(input(\"Enter a number:\"))\n",
    "if isarmstrong(n):\n",
    "    print(\"YES\")\n",
    "else:\n",
    "    print(\"NO\")"
   ]
